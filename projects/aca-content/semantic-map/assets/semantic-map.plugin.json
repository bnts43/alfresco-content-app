{
  "$schema": "../../../../extension.schema.json",
  "$id": "semantic-map.extension",
  "$version": "1.0.0",
  "$name": "Semantic Map Extension",
  "$description": "Interactive Sigma.js graph visualization of document semantic proximity",
  "$license": "LGPL-3.0",
  "$vendor": "Hyland Software, Inc. and its affiliates",

  "routes": [
    {
      "id": "semantic-map.route",
      "path": "semantic-map",
      "layout": "app.layout.main",
      "auth": ["app.auth"],
      "data": {
        "title": "Semantic Map"
      },
      "component": "semantic-map.main.component"
    }
  ],

  "features": {
    "navbar": [
      {
        "id": "semantic-map.nav",
        "order": 500,
        "icon": "bubble_chart",
        "title": "Semantic Map",
        "description": "Explore document semantic proximity",
        "route": "semantic-map",
        "rules": {
          "visible": "semantic-map.canShow"
        }
      }
    ]
  }
}
